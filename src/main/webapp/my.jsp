    <script type="text/javascript">
        $(document).ready(function () {
            function showDiv() {
                $("#linkTitle").removeClass("hidden").addClass("show");
                $("#showLink").css("width", "300px")
                $("#linkDiv").css("display", "block")
            }
            function hiddenDiv() {
                $("#linkTitle").removeClass("show").addClass("hidden");
                $("#showLink").css("width", "50px")
                $("#linkDiv").css("display", "none")

            }
            function handle() {
                var className = $("#linkTitle").attr("class");
                if (className == "show") {
                    hiddenDiv();
                } else {
                    showDiv()
                }
            }
            $("#linkTitle").click(function () {
                handle()
            });
//            $("#linkTitle").hover(function () {
//                handle();
//            });
        });

    </script>

